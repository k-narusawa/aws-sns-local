{
  "name": "aws-sns-local",
  "version": "0.1.0",
  "description": "AWS SNS Mock on local machine",
  "main": "dist/index.ts",
  "type": "commonjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build && node dist/src/server.js",
    "build": "tsc",
    "dev": "ts-node src/server.ts",
    "watch": "ts-node-dev --respawn src/server.ts",
    "clean": "rm -rf dist",
    "format": "npx prettier --config ./.prettierrc --write src/**/*.ts",
    "format:check": "npx prettier --config ./.prettierrc --check src/**/*.ts"
  },
  "author": "k-narusawa",
  "license": "ISC",
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/lodash.find": "^4.6.7",
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.0",
    "@types/xml2js": "^0.4.11",
    "aws-sdk": "^2.1287.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "xml2js": "^0.4.23"
  },
  "dependencies": {
    "lodash.find": "^4.6.0",
    "typescript": "^4.9.4"
  }
}
